{
  "name": "worktime-server",
  "version": "1.0.0",
  "description": "WorkTime Tracker Server with Real-time Sync",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "set NODE_ENV=production && node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "migrate": "ts-node src/database/migrate.ts",
    "migrate:prod": "set NODE_ENV=production && ts-node src/database/migrate.ts",
    "seed": "ts-node src/database/seed.ts",
    "create-test-worker": "ts-node src/database/create-test-worker.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build:only": "tsc"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "expo-server-sdk": "^3.15.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "pg": "^8.11.1",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.18",
    "@types/node": "^20.19.1",
    "@types/pg": "^8.15.4",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "worktime",
    "tracker",
    "gps",
    "sync",
    "websocket"
  ],
  "author": "WorkTime Development Team",
  "license": "MIT"
}
